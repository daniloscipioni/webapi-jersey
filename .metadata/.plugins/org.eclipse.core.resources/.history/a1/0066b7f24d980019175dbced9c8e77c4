package com.schott;

import javax.ws.rs.Path;
import javax.ws.rs.Produces;
import javax.ws.rs.core.MediaType;

@Path("/helloWorld")
public class HelloWordRestService {
		
		@GET
		@Produces(MediaType.TEXT_PLAIN)
		public String getGreeting(){
			String greeting = "Hello ";
			
			return greeting;
		}
}
